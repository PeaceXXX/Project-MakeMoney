[run]
source = app
branch = True
omit =
    */tests/*
    */__pycache__/*
    */migrations/*
    */alembic/*

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
title = Trading Platform - Coverage Report

[xml]
output = coverage.xml

[json]
output = coverage.json
show_contexts = True
